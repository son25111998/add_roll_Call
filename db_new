-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `package_attendance_log`
--

CREATE TABLE `package_attendance_log` (
  `log_id` int(11) NOT NULL,
  `package_id` int(11) NOT NULL COMMENT 'Khóa ngoại đến course_enrollments',
  `student_id` int(11) DEFAULT NULL,
  `attendance_id` int(11) NOT NULL COMMENT 'Khóa ngoại đến attendance',
  `deducted_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `parents`
--

CREATE TABLE `parents` (
  `parent_id` int(11) NOT NULL,
  `name` varchar(255) NOT NULL,
  `phone_number` varchar(20) NOT NULL,
  `discount_type` enum('none','percentage','fixed','free') NOT NULL DEFAULT 'none',
  `discount_value` decimal(10,0) NOT NULL DEFAULT 0,
  `status` enum('pending','approved','rejected') NOT NULL DEFAULT 'pending',
  `tuition_model` enum('monthly','package') NOT NULL DEFAULT 'monthly',
  `balance` decimal(12,0) NOT NULL DEFAULT 0 COMMENT 'Số dư tài khoản, có thể dùng để thanh toán sau',
  `created_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

--
-- Đang đổ dữ liệu cho bảng `parents`
--

