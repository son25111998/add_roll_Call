-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `monthly_invoices`
--

CREATE TABLE `monthly_invoices` (
  `invoice_id` int(11) NOT NULL,
  `student_id` int(11) NOT NULL,
  `month` varchar(7) NOT NULL,
  `sessions_attended` int(11) NOT NULL DEFAULT 0,
  `base_fee` decimal(12,0) NOT NULL DEFAULT 0,
  `other_fees` decimal(12,0) NOT NULL DEFAULT 0,
  `applied_discount` decimal(12,0) NOT NULL DEFAULT 0,
  `final_due_amount` decimal(12,0) NOT NULL DEFAULT 0,
  `amount_paid` decimal(12,0) NOT NULL DEFAULT 0,
  `status` enum('unpaid','paid','overpaid','debt') NOT NULL DEFAULT 'unpaid',
  `created_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

