-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `salary_type`
--

CREATE TABLE `salary_type` (
  `salary_type_id` int(11) NOT NULL,
  `salary_method` enum('percentage','fixed_session','monthly') NOT NULL
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

--
-- Đang đổ dữ liệu cho bảng `salary_type`
--

INSERT INTO `salary_type` (`salary_type_id`, `salary_method`) VALUES
(1, 'percentage'),
(2, 'fixed_session'),
(3, 'monthly');

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `salary_type_history`
--

CREATE TABLE `salary_type_history` (
  `id` int(11) NOT NULL,
  `teacher_id` int(11) NOT NULL,
  `salary_type_id` int(11) NOT NULL,
  `effective_from` date NOT NULL,
  `effective_to` date DEFAULT NULL,
  `salary_value` decimal(12,2) DEFAULT NULL,
  `created_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=utf8 COLLATE=utf8_general_ci;

--
-- Đang đổ dữ liệu cho bảng `salary_type_history`
