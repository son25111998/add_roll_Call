(6, 20, 1000000.00, 'quản lý lớp tốt', '2025-10-01', '2025-10-31', '2025-10-10 03:25:00');

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `teacher_salaries`
--

CREATE TABLE `teacher_salaries` (
  `salary_id` int(11) NOT NULL,
  `teacher_id` int(11) NOT NULL,
  `class_id` int(11) NOT NULL,
  `month` varchar(7) NOT NULL COMMENT 'Định dạng YYYY-MM',
  `class_revenue` decimal(12,0) NOT NULL,
  `total_sessions` int(11) NOT NULL,
  `sessions_taught` int(11) NOT NULL,
  `calculated_salary` decimal(12,0) NOT NULL,
  `calculation_date` timestamp NOT NULL DEFAULT current_timestamp(),
  `salary_type_id` int(11) NOT NULL DEFAULT 1
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

