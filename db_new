) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `course_enrollments`
--

CREATE TABLE `course_enrollments` (
  `package_id` int(11) NOT NULL,
  `student_id` int(11) NOT NULL,
  `parent_id` int(11) NOT NULL,
  `course_name` varchar(255) NOT NULL DEFAULT 'Khóa học',
  `total_fee` decimal(12,0) NOT NULL DEFAULT 0 COMMENT 'Tổng học phí của cả khóa học',
  `amount_paid` decimal(12,0) NOT NULL DEFAULT 0 COMMENT 'Tổng số tiền đã thanh toán',
  `total_sessions` int(11) NOT NULL COMMENT 'Tổng số buổi của khóa học',
  `remaining_sessions` int(11) NOT NULL COMMENT 'Số buổi còn lại đã được kích hoạt',
  `purchase_date` date NOT NULL,
  `is_active` tinyint(1) NOT NULL DEFAULT 1 COMMENT 'Dùng để chỉ trạng thái kích hoạt của cả khóa học',
  `notes` text DEFAULT NULL,
  `status` enum('active','completed','cancelled') NOT NULL DEFAULT 'active'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

--
-- Đang đổ dữ liệu cho bảng `course_enrollments`
--

INSERT INTO `course_enrollments` (`package_id`, `student_id`, `parent_id`, `course_name`, `total_fee`, `amount_paid`, `total_sessions`, `remaining_sessions`, `purchase_date`, `is_active`, `notes`, `status`) VALUES
