-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `payment_installments`
--

CREATE TABLE `payment_installments` (
  `installment_id` int(11) NOT NULL,
  `enrollment_id` int(11) NOT NULL COMMENT 'Khóa ngoại đến course_enrollments',
  `amount_due` decimal(12,0) NOT NULL COMMENT 'Số tiền cần đóng cho đợt này',
  `due_date` date NOT NULL COMMENT 'Hạn cuối cần thanh toán',
  `status` enum('pending','paid','overdue') NOT NULL DEFAULT 'pending' COMMENT 'Trạng thái của đợt thanh toán',
  `payment_id` int(11) DEFAULT NULL COMMENT 'Khóa ngoại đến tuition_payments sau khi đã thanh toán',
  `reminders_enabled` tinyint(1) NOT NULL DEFAULT 1 COMMENT 'Bật (1) / Tắt (0) nhắc nhở tự động cho đợt này',
  `last_reminder_sent_at` datetime DEFAULT NULL COMMENT 'Lưu thời điểm cuối cùng hệ thống gửi tin nhắn nhắc nhở'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

--
-- Đang đổ dữ liệu cho bảng `payment_installments`
--

INSERT INTO `payment_installments` (`installment_id`, `enrollment_id`, `amount_due`, `due_date`, `status`, `payment_id`, `reminders_enabled`, `last_reminder_sent_at`) VALUES
(1, 1, 500000, '2025-12-15', 'paid', 14, 1, NULL),
(2, 1, 500000, '2025-09-16', 'paid', 15, 1, NULL),
(3, 2, 500000, '2025-09-30', 'paid', 28, 1, NULL),
(6, 5, 1000000, '2025-09-19', 'paid', 43, 1, NULL);

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `quizzes`
--

CREATE TABLE `quizzes` (
  `quiz_id` int(11) NOT NULL,
  `teacher_id` int(11) NOT NULL,
  `title` varchar(255) NOT NULL,
  `time_limit_minutes` int(11) NOT NULL,
  `status` enum('draft','published') NOT NULL DEFAULT 'draft',
  `zip_folder_path` varchar(255) NOT NULL,
  `created_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

--
-- Đang đổ dữ liệu cho bảng `quizzes`
--

INSERT INTO `quizzes` (`quiz_id`, `teacher_id`, `title`, `time_limit_minutes`, `status`, `zip_folder_path`, `created_at`) VALUES
(8, 20, 'khao sat chat luong thang 1', 50, 'published', 'uploads/quizzes/8', '2025-09-04 09:00:21'),
(12, 20, 'khao sat chat luong thang 1 (lan 2)', 45, 'published', 'uploads/quizzes/12', '2025-09-09 15:34:10');

-- --------------------------------------------------------

--
-- Cấu trúc bảng cho bảng `quiz_assignments`
--

CREATE TABLE `quiz_assignments` (
  `assignment_id` int(11) NOT NULL,
  `quiz_id` int(11) NOT NULL,
  `student_id` int(11) NOT NULL,
  `exam_code` varchar(50) NOT NULL,
  `json_path` varchar(255) NOT NULL,
  `pdf_path` varchar(255) NOT NULL,
  `assigned_at` timestamp NOT NULL DEFAULT current_timestamp()
) ENGINE=InnoDB DEFAULT CHARSET=latin1 COLLATE=latin1_swedish_ci;

