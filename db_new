--
-- Chỉ mục cho các bảng đã đổ
--

--
-- Chỉ mục cho bảng `absentees`
--
ALTER TABLE `absentees`
  ADD PRIMARY KEY (`absentee_id`),
  ADD KEY `absentees_ibfk_1` (`attendance_id`),
  ADD KEY `absentees_ibfk_2` (`student_id`);

--
-- Chỉ mục cho bảng `admins`
--
ALTER TABLE `admins`
  ADD PRIMARY KEY (`admin_id`),
  ADD UNIQUE KEY `username` (`username`);

--
-- Chỉ mục cho bảng `attendance`
--
ALTER TABLE `attendance`
  ADD PRIMARY KEY (`attendance_id`),
  ADD KEY `class_id` (`class_id`);

--
-- Chỉ mục cho bảng `class`
--
ALTER TABLE `class`
  ADD PRIMARY KEY (`class_id`);

--
-- Chỉ mục cho bảng `class_student_member`
--
ALTER TABLE `class_student_member`
  ADD PRIMARY KEY (`member_id`),
  ADD KEY `class_id` (`class_id`),
  ADD KEY `student_id` (`student_id`);

--
-- Chỉ mục cho bảng `class_teacher_member`
--
ALTER TABLE `class_teacher_member`
  ADD PRIMARY KEY (`member_id`),
  ADD KEY `teacher_id` (`teacher_id`),
  ADD KEY `class_teacher_member_ibfk_1` (`class_id`);

--
-- Chỉ mục cho bảng `class_topics`
--
ALTER TABLE `class_topics`
  ADD PRIMARY KEY (`topic_id`),
  ADD KEY `class_id` (`class_id`);

--
-- Chỉ mục cho bảng `course_enrollments`
--
ALTER TABLE `course_enrollments`
  ADD PRIMARY KEY (`package_id`),
  ADD KEY `student_id` (`student_id`),
  ADD KEY `parent_id` (`parent_id`);

--
-- Chỉ mục cho bảng `file_repository`
--
ALTER TABLE `file_repository`
  ADD PRIMARY KEY (`file_id`),
  ADD UNIQUE KEY `stored_filename` (`stored_filename`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `homework_score`
--
ALTER TABLE `homework_score`
  ADD PRIMARY KEY (`score_id`),
  ADD KEY `homework_score_ibfk_1` (`attendance_id`),
  ADD KEY `homework_score_ibfk_2` (`student_id`);

--
-- Chỉ mục cho bảng `monthly_invoices`
--
ALTER TABLE `monthly_invoices`
  ADD PRIMARY KEY (`invoice_id`),
  ADD KEY `student_id` (`student_id`);

--
-- Chỉ mục cho bảng `on_leave`
--
ALTER TABLE `on_leave`
  ADD PRIMARY KEY (`leave_id`),
  ADD KEY `on_leave_ibfk_1` (`attendance_id`),
  ADD KEY `on_leave_ibfk_2` (`student_id`);

--
-- Chỉ mục cho bảng `other_fees`
--
ALTER TABLE `other_fees`
  ADD PRIMARY KEY (`fee_id`);

--
-- Chỉ mục cho bảng `package_attendance_log`
--
ALTER TABLE `package_attendance_log`
  ADD PRIMARY KEY (`log_id`),
  ADD KEY `fk_log_attendance` (`attendance_id`),
  ADD KEY `fk_log_enrollment_ref_new` (`package_id`);

--
-- Chỉ mục cho bảng `parents`
--
ALTER TABLE `parents`
  ADD PRIMARY KEY (`parent_id`),
  ADD UNIQUE KEY `phone_number` (`phone_number`);

--
-- Chỉ mục cho bảng `payment_installments`
--
ALTER TABLE `payment_installments`
  ADD PRIMARY KEY (`installment_id`),
  ADD KEY `enrollment_id` (`enrollment_id`);

--
-- Chỉ mục cho bảng `quizzes`
--
ALTER TABLE `quizzes`
  ADD PRIMARY KEY (`quiz_id`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `quiz_assignments`
--
ALTER TABLE `quiz_assignments`
  ADD PRIMARY KEY (`assignment_id`),
  ADD UNIQUE KEY `quiz_id` (`quiz_id`,`student_id`),
  ADD KEY `student_id` (`student_id`);

--
-- Chỉ mục cho bảng `quiz_questions`
--
ALTER TABLE `quiz_questions`
  ADD PRIMARY KEY (`id`),
  ADD UNIQUE KEY `quiz_id` (`quiz_id`,`question_uuid`);

--
-- Chỉ mục cho bảng `quiz_results`
--
ALTER TABLE `quiz_results`
  ADD PRIMARY KEY (`result_id`),
  ADD KEY `assignment_id` (`assignment_id`);

--
-- Chỉ mục cho bảng `salary_type`
--
ALTER TABLE `salary_type`
  ADD PRIMARY KEY (`salary_type_id`);

--
-- Chỉ mục cho bảng `salary_type_history`
--
ALTER TABLE `salary_type_history`
  ADD PRIMARY KEY (`id`),
  ADD KEY `salary_type_id` (`salary_type_id`),
  ADD KEY `idx_teacher_date` (`teacher_id`,`effective_from`,`effective_to`);

--
-- Chỉ mục cho bảng `session_homework`
--
ALTER TABLE `session_homework`
  ADD PRIMARY KEY (`homework_id`),
  ADD UNIQUE KEY `unique_attendance_homework` (`attendance_id`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `student`
--
ALTER TABLE `student`
  ADD PRIMARY KEY (`student_id`),
  ADD UNIQUE KEY `email` (`email`),
  ADD KEY `fk_student_parent` (`parent_id`);

--
-- Chỉ mục cho bảng `student_grades`
--
ALTER TABLE `student_grades`
  ADD PRIMARY KEY (`grade_id`),
  ADD UNIQUE KEY `unique_grade` (`student_id`,`class_id`,`grade_type`),
  ADD KEY `class_id` (`class_id`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `student_remarks`
--
ALTER TABLE `student_remarks`
  ADD PRIMARY KEY (`remark_id`),
  ADD UNIQUE KEY `unique_remark_per_student_session` (`attendance_id`,`student_id`),
  ADD KEY `attendance_id` (`attendance_id`),
  ADD KEY `student_id` (`student_id`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `teacher`
--
ALTER TABLE `teacher`
  ADD PRIMARY KEY (`teacher_id`),
  ADD UNIQUE KEY `email` (`email`);

--
-- Chỉ mục cho bảng `teacher_bonus`
--
ALTER TABLE `teacher_bonus`
  ADD PRIMARY KEY (`id`),
  ADD KEY `teacher_id` (`teacher_id`);

--
-- Chỉ mục cho bảng `teacher_salaries`
--
ALTER TABLE `teacher_salaries`
  ADD PRIMARY KEY (`salary_id`),
  ADD UNIQUE KEY `teacher_class_month` (`teacher_id`,`class_id`,`month`),
  ADD KEY `class_id` (`class_id`),
  ADD KEY `fk_salary_type` (`salary_type_id`);

--
-- Chỉ mục cho bảng `tuition_payments`
--
ALTER TABLE `tuition_payments`
  ADD PRIMARY KEY (`payment_id`),
  ADD KEY `parent_id` (`parent_id`),
  ADD KEY `student_id` (`student_id`),
  ADD KEY `invoice_id` (`invoice_id`);

