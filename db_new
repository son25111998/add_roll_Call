--
-- Cấu trúc bảng cho bảng `student_remarks`
--

CREATE TABLE `student_remarks` (
  `remark_id` int(11) NOT NULL,
  `attendance_id` int(11) NOT NULL,
  `student_id` int(11) NOT NULL,
  `teacher_id` int(11) NOT NULL,
  `remark_text` text DEFAULT NULL,
  `score` float DEFAULT NULL,
  `punctuality` varchar(50) DEFAULT 'Đúng giờ' COMMENT 'Đúng giờ',
  `focus_level` varchar(50) DEFAULT 'Tập trung' COMMENT 'Độ tập trung',
  `attitude_level` varchar(10) DEFAULT NULL COMMENT 'Lưu trữ tinh thần học tập',
  `comprehension` varchar(10) DEFAULT '80%' COMMENT 'Mức độ hiểu bài'
) ENGINE=InnoDB DEFAULT CHARSET=utf8mb4 COLLATE=utf8mb4_general_ci;

